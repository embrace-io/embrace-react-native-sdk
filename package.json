{
  "name": "embrace-io",
  "version": "4.2.0",
  "private": "false",
  "author": {
    "name": "Embrace.io",
    "email": "truenorth@embrace.io"
  },
  "homepage": "https://embrace.io/",
  "description": "A React Native wrapper for Embrace SDK",
  "main": "dist/src/embrace.js",
  "types": "dist/src/embrace.d.ts",
  "files": [
    "/dist",
    "/android",
    "/ios",
    "RNEmbrace.podspec",
    "react-native.config.js"
  ],
  "scripts": {
    "publish-modules": "npx lerna run tsc && npx lerna publish --tag-version-prefix='rn-os-modules-'",
    "build:watch": "tsc -w --preserveWatchOutput",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "prebuild": "npm run lint && rm -rf dist",
    "build": "tsc",
    "preversion": "npm run lint && npm run build",
    "version": "git add -A",
    "postversion": "git push && git push --tags",
    "test": "jest",
    "docs": "typedoc --options typedoc.json --inputFiles ./src/embrace.ts && touch ./docs/.nojekyll && git add docs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn docs && lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "yarn prettier",
      "yarn lint"
    ]
  },
  "license": "ISC",
  "keywords": [
    "react-native",
    "react-component",
    "tracking",
    "embrace",
    "bug tracker"
  ],
  "peerDependencies": {
    "react-native": ">=0.56.0"
  },
  "dependencies": {
    "glob": "^7.1.4",
    "gzip-js": "^0.3.2",
    "inquirer": "^6.5.1",
    "promise": "^8.0.3",
    "semver": "^7.1.3",
    "xcode": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/gzip-js": "^0.3.3",
    "@types/jest": "^28.1.7",
    "@types/node": "^12.7.2",
    "@types/react-native": "^0.60.2",
    "@types/semver": "^7.1.0",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "babel-jest": "^25.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-native": "^4.1.0",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "lerna": "^6.0.3",
    "lint-staged": "^10.1.7",
    "metro-react-native-babel-preset": "^0.77.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.56.0",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^25.3.1",
    "typedoc": "^0.17.8",
    "typescript": "^4.7.4"
  },
  "rnpm": {
    "commands": {
      "postlink": "node node_modules/embrace-io/dist/scripts/postlink"
    }
  },
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:embrace-io/embrace-io.git"
  }
}
