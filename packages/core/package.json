{
  "name": "@embrace-io/react-native",
  "version": "5.0.0-alpha.0",
  "description": "A React Native wrapper for the Embrace SDK",
  "dependencies": {
    "glob": "^7.1.4",
    "gzip-js": "^0.3.2",
    "inquirer": "^6.5.1",
    "promise": "^8.0.3",
    "semver": "^7.1.3",
    "xcode": "^2.0.0"
  },
  "peerDependencies": {
    "react-native": ">=0.56.0"
  },
  "files": [
    "lib",
    "android",
    "ios",
    "RNEmbrace.podspec"
  ],
  "author": "Embrace <support@embrace.io> (https://embrace.io/)",
  "bugs": {
    "url": "https://github.com/embrace-io/embrace-react-native-sdk/issues",
    "email": "support@embrace.io"
  },
  "homepage": "https://github.com/embrace-io/embrace-react-native-sdk",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/embrace-io/embrace-react-native-sdk",
    "directory": "packages/core"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/src/index.js",
  "typings": "lib/src/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "keywords": [
    "embrace",
    "react-native",
    "tracking",
    "observability",
    "instrumentation",
    "telemetry",
    "bug tracker"
  ],
  "scripts": {
    "build": "tsc",
    "ios:build:clean": "xcodebuild clean -workspace test-project/ios/RNEmbraceTestProject.xcworkspace -scheme Pods-RNEmbrace-RNEmbraceTests",
    "ios:scheme": "xcodebuild -workspace test-project/ios/RNEmbraceTestProject.xcworkspace -list",
    "ios:test": "xcodebuild test -workspace test-project/ios/RNEmbraceTestProject.xcworkspace -scheme Pods-RNEmbrace-RNEmbraceTests -destination 'platform=iOS Simulator,name=iPhone 15 Pro'"
  },
  "devDependencies": {
    "typescript": "^4.7.4"
  }
}
