{
  "name": "@embrace-io/react-native-otlp",
  "version": "5.0.1",
  "private": true,
  "description": "A React Native wrapper for the Embrace SDK that allows to connect to OTLP endpoints",
  "files": [
    "lib",
    "android",
    "ios",
    "RNEmbraceOTLP.podspec"
  ],
  "author": "Embrace <support@embrace.io> (https://embrace.io/)",
  "bugs": {
    "url": "https://github.com/embrace-io/embrace-react-native-sdk/issues",
    "email": "support@embrace.io"
  },
  "homepage": "https://github.com/embrace-io/embrace-react-native-sdk",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/embrace-io/embrace-react-native-sdk",
    "directory": "packages/react-native-otlp"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/src/index.js",
  "typings": "lib/src/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "keywords": [
    "embrace",
    "react-native",
    "tracking",
    "observability",
    "instrumentation",
    "telemetry",
    "bug tracker",
    "otlp"
  ],
  "scripts": {
    "prebuild": "cp -r ../../interfaces/ ./src/interfaces/common",
    "build": "tsc",
    "ios:schemes": "xcodebuild -workspace test-project/ios/RNEmbraceOTLPTest.xcworkspace -list",
    "ios:install": "cd test-project && yarn && cd ios && pod install && rm .xcode.env.local",
    "ios:test": "../../scripts/run-ios-tests.sh test-project/ios/RNEmbraceOTLPTests.xcworkspace RNEmbraceOTLPTests"
  },
  "peerDependencies": {
    "react-native": ">=0.56.0"
  },
  "devDependencies": {
    "typescript": "^5.6.3"
  },
  "embrace": {
    "iosVersion": "6.5.1",
    "androidVersion": "6.13.0"
  }
}
